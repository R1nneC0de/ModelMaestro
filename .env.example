# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=./gcp-credentials/gcp-key.json
GCS_BUCKET_NAME=agentic-platform-data

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key

# Application Configuration
ENVIRONMENT=development
DEBUG=true
API_V1_PREFIX=/api/v1
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# File Upload Configuration
MAX_UPLOAD_SIZE_MB=10240
ALLOWED_EXTENSIONS=.csv,.json,.jpg,.jpeg,.png,.txt

# Data Analysis Configuration
VALIDATION_SAMPLE_SIZE=5
ANALYSIS_SAMPLE_SIZE=50

# Training Configuration
MAX_TRAINING_HOURS=24
MAX_ITERATIONS=5
MAX_CONCURRENT_PROJECTS_PER_USER=5

# Security Configuration
DATA_RETENTION_DAYS=90
